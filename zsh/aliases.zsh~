# Colorize grep
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

# Standard Aliases
alias la='ls -A'
alias l='ls -CF'
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias ll='ls -lGaFh'


# Oscar Aliases from jjacob

# SequelPro alias
#alias db="LIBMYSQL_ENABLE_CLEARTEXT_PLUGIN=y /Applications/Sequel\ Pro.app/Contents/MacOS/Sequel\ Pro"


# GITHUB ALIASES
function gs() { git status ;}
function ga() { git add "$@" ;}
function gaa() { git add -A ;}

function gb() { git branch ;}

function gcm() { git commit -m "$1" ;}
function gcam() { git commit -am "$1" ;}

function gf() { git fetch ;}

function gco() { git checkout "$1" ;}
function gchm() { git checkout master ;}
function gcb() { git checkout -b "$1";}
function gchmgf() { git checkout master; git fetch ;}

function gd() { git diff ;}

# PANTS ALIASES
function pt() { ./pants test "$1" ;}
function pgpp() { ./pants gen-protobuf-py protobuf/src:: ;}

# ARC ALIASES
function al() { arc lint "$@" ;}
function ad() { arc diff ;}
function adp() { arc diff --preview ;}

# KAURORA ALIASES
function ki() { kinit npuljic ;}

# NORMAL COMMAND ALIASES
function h() { history ;}
function cdd() { cd ~/workspace/data ;}
function cdh() { cd ~/ ;}

